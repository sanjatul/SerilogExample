{
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "WriteTo": [
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args": {
            "path": "logs/myCrazyConffigLog-.txt",
            "rollingInterval": "Day"
          }
        }
      ]
    }
  ,
  "AllowedHosts": "*"
}
